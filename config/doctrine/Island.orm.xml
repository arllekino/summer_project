<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
    xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Entity\Island" table="island">

        <id name="id" type="integer" column="island_id">
            <generator strategy="IDENTITY" />
        </id>
        
        <field name="foodCount" column="food" type="integer" nullable="false" />   
        <field name="maxFoodCount" column="max_food" type="integer" nullable="false" />   
        <field name="woodCount" column="wood" type="integer" nullable="false" />   
        <field name="maxWoodCount" column="max_wood" type="integer" nullable="false" />   
        <field name="stonesCount" column="stones" type="integer" nullable="false" />   
        <field name="maxStonesCount" column="max_stones" type="integer" nullable="false" />   
        <field name="warriorsCount" column="warriors" type="integer" nullable="false" />   
        <field name="maxWarriorsCount" column="max_warriors" type="integer" nullable="false" />   
        <field name="villagersCount" column="villagers" type="integer" nullable="false" />   
        <field name="hammersCount" column="hammers" type="integer" nullable="false" />   
        <field name="moneyCount" column="money" type="integer" nullable="false" />   
        <field name="knowledgeCount" column="knowledge" type="integer" nullable="false" />  
        <field name="userId" column="user_id" type="integer" nullable="false"/>
        <field name="keyRoom" column="key_room" type="string" nullable="false"/>


    </entity>

</doctrine-mapping>