<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
    xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Entity\Lobby" table="lobby">

        <unique-constraints>
            <unique-constraint columns="player_id_1" name="search_id1" />
            <unique-constraint columns="player_id_2" name="search_id2" />
            <unique-constraint columns="player_id_3" name="search_id3" />
            <unique-constraint columns="player_id_4" name="search_id4" />
        </unique-constraints>

        <id name="lobbyId" type="integer" column="lobby_id">
            <generator strategy="IDENTITY" />
        </id>
        <field name="keyRoom" column="key_room" type="string" nullable="true" />   
        <field name="playerId1" column="player_id_1" type="integer" nullable="true" />   
        <field name="playerId2" column="player_id_2" type="integer" nullable="true" />   
        <field name="playerId3" column="player_id_3" type="integer" nullable="true" />   
        <field name="playerId4" column="player_id_4" type="integer" nullable="true" />   

    </entity>

</doctrine-mapping>